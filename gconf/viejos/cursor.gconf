<gconfentryfile>
  <entrylist base="/desktop/gnome/peripherals/mouse">
    <entry>
      <key>cursor_font</key>
      <schema_key>/schemas/desktop/gnome/peripherals/mouse/cursor_font</schema_key>
    </entry>
    <entry>
      <key>cursor_size</key>
      <schema_key>/schemas/desktop/gnome/peripherals/mouse/cursor_size</schema_key>
      <value>
        <int>32</int>
      </value>
    </entry>
    <entry>
      <key>cursor_theme</key>
      <schema_key>/schemas/desktop/gnome/peripherals/mouse/cursor_theme</schema_key>
      <value>
        <string>ComixCursors-White-Small-Slim</string>
      </value>
    </entry>
    <entry>
      <key>double_click</key>
      <schema_key>/schemas/desktop/gnome/peripherals/mouse/double_click</schema_key>
      <value>
        <int>400</int>
      </value>
    </entry>
    <entry>
      <key>drag_threshold</key>
      <schema_key>/schemas/desktop/gnome/peripherals/mouse/drag_threshold</schema_key>
      <value>
        <int>8</int>
      </value>
    </entry>
    <entry>
      <key>left_handed</key>
      <schema_key>/schemas/desktop/gnome/peripherals/mouse/left_handed</schema_key>
      <value>
        <bool>false</bool>
      </value>
    </entry>
    <entry>
      <key>locate_pointer</key>
      <schema_key>/schemas/desktop/gnome/peripherals/mouse/locate_pointer</schema_key>
      <value>
        <bool>false</bool>
      </value>
    </entry>
    <entry>
      <key>motion_acceleration</key>
      <schema_key>/schemas/desktop/gnome/peripherals/mouse/motion_acceleration</schema_key>
      <value>
        <float>-1</float>
      </value>
    </entry>
    <entry>
      <key>motion_threshold</key>
      <schema_key>/schemas/desktop/gnome/peripherals/mouse/motion_threshold</schema_key>
      <value>
        <int>-1</int>
      </value>
    </entry>
    <entry>
      <key>single_click</key>
      <schema_key>/schemas/desktop/gnome/peripherals/mouse/single_click</schema_key>
      <value>
        <bool>true</bool>
      </value>
    </entry>
  </entrylist>
</gconfentryfile>
